<% breadcrumb :establishment, @establishment %>
  <div class="row q-height nopadding">
    <% if @establishment.map %>
    <div class="" id="well3">
            <iframe
              width="100%"
              height="100%"
              frameborder="0" style="border:0"
              src="https://www.google.com/maps/embed/v1/search?key=AIzaSyCnP6SykQuPfIT2EIr2jAmT_KU2xV2U_vQ &q=<%= @establishment.geo_address %>">
            </iframe>
          </div>
          <% else %>

            <p>NO MAP</p>
            
          <% end %>
  </div>
  <div class="container text-center">
    <div class="row somepadding">
      <div class="col-md-6">
        <h1><%= @establishment.name %></h1>
        <h3><%= @establishment.address %></h3>
        <h3><%= @split_url[0] %></h3>
      
      </div>
      <div class="col-md-6 somepadding">
        
          <p><%= @average_rating %></p>
      <% if @reviews.blank? %>
        <h3>No reviews yet. Be the first to write one!</h3>
      <% else %>
        <% @reviews.each do |review| %>
          <p><%= review.comment %></p>
        <% end %>
      <% end %>

      <%= link_to "Write Review", new_establishment_review_path(@establishment), class: "btn btn-primary" %>
   
      </div>
    </div>
  </div>
  
  



