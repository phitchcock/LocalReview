
  <div class="row q-height nopadding">
    <% if @establishment.map %>
    <div class="" id="well3">
            <iframe
              width="100%"
              height="100%"
              frameborder="0" style="border:0"
              src="https://www.google.com/maps/embed/v1/place?key=AIzaSyCnP6SykQuPfIT2EIr2jAmT_KU2xV2U_vQ &q=loc%3A+F+Street+at+%31%32th+Street+Sacramento+%28downtown%29+CA+US">
            </iframe>
          </div>
          <% else %>

            <p>NO MAP</p>
            
          <% end %>
  </div>
  <div class="container">
    <div class="row">
      <div class="col-md-6">
        <h1><%= @establishment.name %></h1>
        <h3><%= @establishment.address %></h3>
        <h3><%= @split_url[0] %></h3>
      </div>
      <div class="col-md-6">
        <div class="col-md-6">
      <% if @reviews.blank? %>
        <h3>No reviews yet. Be the first to write one!</h3>
      <% else %>
        <% @reviews.each do |review| %>
          <p><%= review.rating %></p>
          <p><%= review.comment %></p>
        <% end %>
      <% end %>

      <%= link_to "Write Review", new_establishment_review_path(@establishment), class: "btn btn-primary" %>
    </div>
      </div>
    </div>
  </div>
  
  



