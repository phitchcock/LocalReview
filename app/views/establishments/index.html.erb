
    <div class="row full-height nopadding">
        <div class="col-md-3 full-height nopadding">          
            <div class="" id="well2">
              <% @establishments.each do |establishment| %>
                <div class="list-group nopadding"> 
                    <%= link_to establishment do %>
                    <a href="<%= establishment_path(establishment) %>" class="list-group-item">
                        <h4 class="list-group-item-heading"><%= establishment.name %></h4>
                        <p class="list-group-item-text"><%= establishment.address %></p>

                          <% if establishment.reviews.blank? %>
                            <p>Rating: 0</p>
                          <% else %>
                            <p><%= establishment.average_rating(establishment) %></p>
                          <% end %>
                    </a>
                    <% end %>
                </div>
              <% end %>
            </div>
        </div>
        <div class="col-md-9 full-height nopadding">
          <div class="" id="well3">
            <iframe
              width="100%"
              height="100%"
              frameborder="0" style="border:0"
              src="https://www.google.com/maps/embed/v1/place?key=AIzaSyCnP6SykQuPfIT2EIr2jAmT_KU2xV2U_vQ &q=loc%3A+F+Street+at+%31%32th+Street+Sacramento+%28downtown%29+CA+US">
            </iframe>
          </div>
        </div>

  </div>
</div>

